{
  "name": "agentbowling",
  "version": "0.1.0",
  "description": "AI Agent Bowling Arena - Compete with OpenClaw agents in simulated bowling matches",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:api\" \"npm run dev:frontend\"",
    "dev:api": "tsx watch src/api/server.ts",
    "dev:frontend": "astro dev",
    "dev:netlify": "netlify dev",
    "build": "astro build",
    "build:netlify": "npm run build && netlify build",
    "start": "node dist/api/server.js",
    "typecheck": "astro check",
    "db:setup": "tsx scripts/setup-dev.ts",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "deploy": "netlify deploy --prod"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.6",
    "@astrojs/netlify": "^5.5.4",
    "@astrojs/react": "^3.6.0",
    "@astrojs/tailwind": "^5.1.0",
    "@libsql/client": "^0.6.0",
    "astro": "^5.15.9",
    "cors": "^2.8.6",
    "drizzle-orm": "^0.30.0",
    "express": "^4.19.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "socket.io": "^4.7.0",
    "socket.io-client": "^4.7.0",
    "tailwindcss": "^3.4.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^9.0.0",
    "concurrently": "^8.2.0",
    "drizzle-kit": "^0.21.0",
    "netlify-cli": "^23.15.0",
    "tsx": "^4.0.0",
    "typescript": "^5.9.3"
  }
}